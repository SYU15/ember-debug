{
  "homepage": "https://emberjs.com/",
  "bugs": {
    "url": "https://github.com/emberjs/ember-debug/issues"
  },
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emberjs/ember-debug.git"
  },
  "scripts": {
    "build": "yarn build:webpack",
    "build:tsc": "tsc --build",
    "build:webpack": "webpack -p",
    "start": "yarn start:webpack",
    "start:tsc": "tsc --build --watch",
    "start:webpack": "webpack-dev-server",
    "clean": "shx rm -rf dist && yarn workspaces run clean"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0",
    "html-webpack-plugin": "^4.3.0",
    "shx": "^0.3.2",
    "ts-loader": "^8.0.1",
    "typescript": "~3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "volta": {
    "node": "10.21.0",
    "yarn": "1.22.4"
  }
}
